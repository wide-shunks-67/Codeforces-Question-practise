#include<iostream>#include<vector>#include<string>using namespace std;int main(){    ios_base::sync_with_stdio(false);    cin.tie(NULL);    int t;    cin>>t;    while(t--){        int n,k;        cin>>n>>k;        vector<int> arr(n);        bool possible = false;        for(int i=0;i<n;i++){            cin>>arr[i];            if(arr[i]==k){                possible = true;            }        }        if(possible){            cout<<"YES\n";        } else {            cout<<"NO\n";        }    }    return 0;}